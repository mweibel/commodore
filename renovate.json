{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":disableDependencyDashboard"
  ],
  "python": {
    "enabled": false
  },
  "poetry": {
    "enabled": true
  },
  "lockFileMaintenance": {
    "enabled": true
  },
  "ignoreDeps": [
    "gitpython",
    "oauthlib",
    "pyjwt",
    "requests"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["cookiecutter"],
      "allowedVersions": "<2.0"
    }
  ],
  "labels": [
    "dependency"
  ]
}
